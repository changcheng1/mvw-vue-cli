  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "inquirer": "^8.2.6",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^6.3.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
